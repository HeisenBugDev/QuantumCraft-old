package sammko.quantumCraft;

import sammko.quantumCraft.core.QuantumCraft;
import sammko.quantumCraft.items.ItemInitializator;
import net.minecraft.src.Block;
import net.minecraft.src.CreativeTabs;
import cpw.mods.fml.common.Side;
import cpw.mods.fml.common.asm.SideOnly;

final class QuantumCraftCreativeTab extends CreativeTabs
{
	QuantumCraftCreativeTab(String par2Str)
    {
        super(par2Str);
    }

    @SideOnly(Side.CLIENT)

    /**
     * the itemID for the item to be displayed on the tab
     */
    public int getTabIconItemIndex()
    {
        return ItemInitializator.EmptyBlock.blockIndexInTexture;
    }
}
